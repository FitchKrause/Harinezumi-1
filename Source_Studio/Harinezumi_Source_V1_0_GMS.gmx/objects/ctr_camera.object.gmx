    view_yhalf      = global.display_height * 0.5;
    
 // Horizontal Borders:
    border_left  = x-8;
    border_right = x+8;
    
 // Horizontal limits:
    limit_left  = 0;
    limit_right = room_width;
    
 // Vertical Borders:
    border_top    = y-32;
    border_bottom = y+32;
    
 // Vertical limits:
    limit_top    = 0;
    limit_bottom = room_height;
    
 // Camera handle:
    cam_handle = obj_character;
    cam_speed  = 6.5;
    
 // Camera position.
    cam_x       = x;
    cam_y       = y;
    cam_x_shift = 0;
    cam_y_shift = 0;
    cam_lag     = 0;
    cam_limit   = 0;
