            <string>/// Magnetized.
 // Checks if the character has a lightning shield. If they do we replace this ring with a magnetized one.
 
 // Exit if in motion.
    if(motion == true) exit;

    if(magnetized == false)
    {
       if(!instance_exists(par_character)) exit;
       
       var character;
       character = instance_nearest(x, y, par_character);
       
       if(character != noone)
       {
          if(character.shield_index == SHIELD_LIGHTNING)
          {
             if(distance_to_object(character) &lt; 64)
             {
                magnetized = true;
                instance_destroy();
             }
          }
       }
    }
